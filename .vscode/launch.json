    "version": "0.2.0",
    "configurations": [

        {
          "name": "web_flutter",
          "request": "launch",
          "type": "dart",
          
          "args": ["--web-hostname=localhost", "--web-port=5000"]
        },
        {
          "name": "web_flutter (profile mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "profile",
          "args": ["--web-hostname=localhost", "--web-port=5000"]
        },
        {
          "name": "web_flutter (release mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "release",
          "args": ["--web-hostname=localhost", "--web-port=5000"]
        }
    ]
}